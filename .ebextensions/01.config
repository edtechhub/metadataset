Resources:
  AWSEBAutoScalingGroup:
    Metadata:
      AWS::CloudFormation::Authentication:
        S3Auth:
          type: "s3"
          buckets: ["metadataset-config"]
          roleName: "aws-elasticbeanstalk-ec2-role"

files:
  "/tmp/config.json" :
    mode: "000644"
    owner: root
    group: root
    authentication: "S3Auth"
    source: https://s3-eu-west-2.amazonaws.com/metadataset-config/config.json
